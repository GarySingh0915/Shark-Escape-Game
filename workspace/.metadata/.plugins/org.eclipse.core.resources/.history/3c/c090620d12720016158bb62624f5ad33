	JOptionPane.showMessageDialog(null,"*32 bit - intOne = "+intOne+"\t*");
	JOptionPane.showMessageDialog(null,"*64 bit - longOne = "+longOne+"\t*");
	JOptionPane.showMessageDialog(null,"*                               *");
	JOptionPane.showMessageDialog(null,"*         real types            *");
	JOptionPane.showMessageDialog(null,"*                               *");
	JOptionPane.showMessageDialog(null,"*32 bit - floatOne = "+floatOne+"\t*");
	JOptionPane.showMessageDialog(null,"*64 bit - doubleOne = "+doubleOne+"\t*");
	JOptionPane.showMessageDialog(null,"*                               *");
	JOptionPane.showMessageDialog(null,"*      other integer types      *");
	JOptionPane.showMessageDialog(null,"*                               *");
	JOptionPane.showMessageDialog(null,"*16 bit - charOne = "+charOne+"\t\t*");
	JOptionPane.showMessageDialog(null,"*                               *");
	JOptionPane.showMessageDialog(null,"*         other types           *");
	JOptionPane.showMessageDialog(null,"*                               *");
	JOptionPane.showMessageDialog(null,"*booleanOne = "+booleanOne+"\t\t*");
	JOptionPane.showMessageDialog(null,"*stringOne = "+stringOne+"\t*");
	JOptionPane.showMessageDialog(null,"\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\");
	
	
	
	
/*	
